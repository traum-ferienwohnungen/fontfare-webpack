{
  "name": "@traum-ferienwohnungen/nuxt-fontfare",
  "version": "1.0.0",
  "description": "The nuxt version of fontfare",
  "main": "module.js",
  "scripts": {
    "update": "node scripts/index.js",
    "prepublishOnly": "npm run update",
    "postpublish": "rm -rf fonts",
    "preversion": "npm run update",
    "version": "git add -f -A fonts",
    "postversion": "git push && git push --tags && rm -rf fonts"
  },
