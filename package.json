    "type": "git",
    "url": "git+https://github.com/traum-ferienwohnungen/nuxt-fontfare.git"
  },
  "keywords": [
    "font",
    "iconfont",
    "icons",
    "tfw",
    "nuxt",
    "nuxtjs",
    "module"
  ],
  "author": "Johannes Konze <konze@traum-ferienwohnungen.de>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/traum-ferienwohnungen/nuxt-fontfare/issues"
  },
  "homepage": "https://github.com/traum-ferienwohnungen/nuxt-fontfare#readme",
  "devDependencies": {
    "adm-zip": "^0.4.13",
    "axios": "^0.21.1"
  }
}
